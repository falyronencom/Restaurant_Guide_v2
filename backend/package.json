{
  "name": "restaurant-guide-belarus-backend",
  "version": "1.0.0",
  "description": "Backend API for Restaurant Guide Belarus - Node.js/Express server with PostgreSQL and Redis",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "test": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:verbose": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\"",
    "seed": "node scripts/seed-establishments.js",
    "seed:reviews": "node scripts/seed-reviews.js",
    "clear-data": "node scripts/clear-establishments.js",
    "clear:reviews": "node scripts/clear-reviews.js --force",
    "count": "node scripts/count-establishments.js",
    "db:reset": "node scripts/clear-reviews.js --force && node scripts/clear-establishments.js --force && node scripts/seed-establishments.js && node scripts/seed-reviews.js"
  },
  "keywords": [
    "restaurant",
    "guide",
    "belarus",
    "api",
    "express"
  ],
  "author": "Restaurant Guide Belarus Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "argon2": "^0.31.1",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "uuid": "^13.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "eslint": "^8.54.0",
    "jest": "^30.2.0",
    "prettier": "^3.1.0",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
